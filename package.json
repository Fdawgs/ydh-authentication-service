  "author": "Frazer Smith",
  "license": "MIT",
  "devDependencies": {
    "eslint": "^6.0.1",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-nhsuk": "^0.17.0",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-json": "^1.4.0",
    "eslint-plugin-mocha": "^5.3.0",
    "nodemon": "^1.19.1"
  },
  "dependencies": {
    "compression": "^1.7.4",
    "express": "^4.17.1",
    "helmet": "^3.18.0",
    "request": "^2.88.0",
    "winser": "^1.0.3",
    "fhir-stu3-subscription-resthook": "git+https://github.com/BlackPearSw/fhir-stu3-subscription-resthook.git"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Fdawgs/ydh-sider-authentication-service.git"
  },
